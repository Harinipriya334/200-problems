class Main {
    public static void main(String[] args) {
        int n=3;
        for(int i=1;i<=n;i++){
           System.out.print("{} ");
           
        for(int j=2;j<=i;j++){
            for(int m=1;m<=j;m++){
            System.out.print('{');
            }
            for(int m=1;m<=j;m++){
            System.out.print('}');
            }
             
            System.out.print(" ");
        }
         System.out.println();
        }
    }
}
